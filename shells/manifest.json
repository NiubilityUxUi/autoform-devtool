{
    "name": "AutoForm devtools",
    "version": "1.0.1",
    "description": "",
    "manifest_version": 2,
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
  
    "browser_action": {
      "default_icon": {
        "16": "icons/16g.png",
        "48": "icons/48g.png",
        "128": "icons/128g.png"
      },
      "default_title": "test Devtools",
      "default_popup": "popups/not-found.html"
    },
  
    "permissions": [
      "http://*/*",
      "https://*/*",
      "file:///*"
    ],
  
    "web_accessible_resources": [ "devtools.html" ],
  
    "devtools_page": "devtools.html",
  
    "background": {
      "scripts": [ "dist/background.js" ],
      "persistent": false
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["dist/hook.js"],
        "run_at": "document_start"
      }
    ]
  }
  